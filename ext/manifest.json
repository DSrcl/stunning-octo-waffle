{
  "manifest_version": 2,

  "name": "History Search",
  "description": "This will get us an A",
  "version": "1.0",

  "background": {
	  "scripts": ["main.js"]
  },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "webNavigation",
	"http://*/*",
	"https://*/*" 
   ],
   "chrome_url_overrides": {
	   "history": "index.html"
   }
}

